<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff" >

    <RelativeLayout
        android:id="@+id/fragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <RelativeLayout
            android:id="@+id/topMenu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#f8f8f8"
            android:paddingBottom="5dp"
            android:paddingTop="5dp" >

            <Button
                android:id="@+id/btnMenu"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/heade_btn_Height"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:background="@drawable/sidemenu" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginTop="8dp"
                android:text="@string/app_title"
                android:textSize="20sp" />
        </RelativeLayout>

        <View
            android:id="@+id/topMenuBorder"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/topMenu"
            android:background="#bbbbbb" />

        <ScrollView
            android:id="@+id/menuScroolView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bottomMenu"
            android:layout_below="@+id/topMenuBorder"
            android:background="#fff" >

            <LinearLayout
                android:id="@+id/menuContentWrapper"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <!-- Row One Start -->

                <LinearLayout
                    android:id="@+id/row1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:baselineAligned="false" >

                    <RelativeLayout
                        android:id="@+id/btnSchedule"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/schedule"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvScheduleText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Schedule"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnDiagnosis"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/diagnosis"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvDiagnosisText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Diagnosis"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnTreatment"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="4dp"
                        android:layout_weight="1"
                        android:background="@drawable/treatment"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvTreatmentText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Treatment"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>
                </LinearLayout>
                <!-- Row One End -->
                <!-- Row Two Start -->

                <LinearLayout
                    android:id="@+id/row2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:baselineAligned="false" >

                    <RelativeLayout
                        android:id="@+id/btnImages"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/images"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvImagesText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Images"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnMsrelapesExacerbation"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/ms_relapes_exacerbation"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvMsrelapesExacerbationText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:gravity="center_horizontal"
                            android:text="@string/MS_Relapes"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnLabs"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="4dp"
                        android:layout_weight="1"
                        android:background="@drawable/labs"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvLabsText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Labs"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>
                </LinearLayout>
                <!-- Row Two End -->
                <!-- Row Three Start -->

                <LinearLayout
                    android:id="@+id/row3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:baselineAligned="false" >

                    <RelativeLayout
                        android:id="@+id/btnMsProfile"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/ms_profile"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvMsProfileText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/MS_Profile"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnMsResources"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/ms_resources"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvMsResources"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:gravity="center_horizontal"
                            android:text="@string/MS_Resources"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnAboutMs"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="4dp"
                        android:layout_weight="1"
                        android:background="@drawable/about_ms"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvAboutMs"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/About_Ms"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>
                </LinearLayout>
                <!-- Row Three End -->
                <!-- Row Four Start -->

                <LinearLayout
                    android:id="@+id/row4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:baselineAligned="false" >

                    <RelativeLayout
                        android:id="@+id/btnInjectionTracker"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/injection_tracker"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvInjectionTracker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:gravity="center_horizontal"
                            android:text="@string/Injection_Tracker"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/btnSymptoms"
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/symptoms"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvSymptoms"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:text="@string/Symptoms"
                            android:textSize="@dimen/home_tv_textsize" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="@dimen/home_btn_Width"
                        android:layout_height="@dimen/home_btn_Height"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/empty"
                        android:paddingBottom="8dp" >

                        <TextView
                            android:id="@+id/tvMsProfileText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true" />
                    </RelativeLayout>
                </LinearLayout>
                <!-- Row Two End -->
            </LinearLayout>
        </ScrollView>
        <!-- Bottom menu start -->
    </RelativeLayout>

    <include
        android:id="@+id/bottomMenu"
        layout="@layout/bottom_menu" />
    <!-- Bottom menu end -->

</RelativeLayout>